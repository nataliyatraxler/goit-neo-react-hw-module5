{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/:1"
    }
  ],
  "redirects": [
    {
      "source": "/search",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
